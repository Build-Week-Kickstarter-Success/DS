    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: ETL update",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["reset"]
        },

        {
            "name": "Python: flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.wsgi",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run"
            ],
            "console": "integratedTerminal"
        }
    ]
}